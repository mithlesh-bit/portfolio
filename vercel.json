{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/count",
            "dest": "src/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}