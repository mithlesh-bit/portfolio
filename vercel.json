{
    "version": 2,
    "builds": [
        {
            "src": "./src/index.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1",
            "continue": true
        }
    ],
    "env": {
        "dburl": "mongodb+srv://mithlesh:1234@cluster0.tnbojkw.mongodb.net/newportfolio",
        "port": 8000
    }
}